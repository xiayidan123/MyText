// 验证码倒计时按钮

#import <UIKit/UIKit.h>

@interface OMCodeCountdownButton : UIButton

/** 倒计时总时长 */
@property (assign, nonatomic) NSTimeInterval duration;

/** 开始倒计时 */
- (void)fire;

/** 结束倒计时 */
- (void)stop;



@end
